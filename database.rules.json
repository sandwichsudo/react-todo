{
  "rules": {
      "users": {
          ".read": "root.child('teams').child('tvx-0001').child('admin').val() == auth.uid",
          "$uid": {
              ".read": "auth.uid === $uid",
              ".write": "auth.uid === $uid"
          }
      },
      "teams": {
        ".write": "false",
        ".read": "true"
      }
  }
}
